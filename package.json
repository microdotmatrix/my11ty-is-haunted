{
  "name": "eleventy-starter-ghost",
  "version": "1.0.0",
  "description": "A starter template to build websites with Ghost & Eleventy",
  "main": ".eleventy.js",
  "scripts": {
    "start": "yarn run dev",
    "dev": "cross-env ELEVENTY_ENV=dev eleventy --serve",
    "build": "cross-env ELEVENTY_ENV=prod eleventy",
    "test": "yarn run build",
    "lint": "eslint . --ext .js --cache",
    "dev:assets": "webpack --mode=development",
    "prod:assets": "webpack --mode=production",
    "style": "webpack --watch --mode=development",
    "watch": "npm-run-all --parallel dev style",
    "deploy": "npm-run-all --parallel build prod:assets"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TryGhost/eleventy-starter-ghost.git"
  },
  "keywords": [
    "eleventy",
    "ghost",
    "starter",
    "boilerplate",
    "static",
    "site",
    "jamstack"
  ],
  "author": "David Darnes",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TryGhost/eleventy-starter-ghost/issues"
  },
  "homepage": "https://github.com/TryGhost/eleventy-starter-ghost#readme",
  "dependencies": {
    "accoutrement": "^4.0.1",
    "bootstrap-icons": "^1.8.1",
    "gerillass": "^1.3.0",
    "gorko": "^0.9.1",
    "gsap": "^3.10.2",
    "jquery": "^3.6.0",
    "nord": "^0.2.1",
    "open-props": "^1.3.16"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.11.0",
    "@11ty/eleventy-plugin-rss": "^1.1.1",
    "@babel/core": "^7.17.8",
    "@babel/preset-env": "^7.16.11",
    "@babel/register": "^7.17.7",
    "@tryghost/admin-api": "^1.8.0",
    "@tryghost/content-api": "^1.4.4",
    "@tryghost/helpers": "^1.1.6",
    "autoprefixer": "10.4.4",
    "browser-sync": "^2.27.9",
    "axios": "^0.21.2",
    "babel-loader": "^8.2.2",
    "browser-sync-webpack-plugin": "^2.3.0",
    "copy-webpack-plugin": "^10.2.4",
    "cross-env": "^7.0.0",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^3.4.1",
    "cssnano": "^4.1.10",
    "del": "^6.0.0",
    "dotenv": "^8.2.0",
    "eleventy-plugin-lazyimages": "liamfiddler/eleventy-plugin-lazyimages",
    "eleventy-plugin-local-images": "^0.4.0",
    "eslint": "7.32.0",
    "eslint-plugin-ghost": "2.11.0",
    "eslint-utils": "3.0.0",
    "groq": "^0.140.0",
    "html-minifier": "^4.0.0",
    "html-minimizer-webpack-plugin": "^3.5.0",
    "luxon": "^1.13.2",
    "mini-css-extract-plugin": "^2.6.0",
    "npm-run-all": "^4.1.5",
    "postcss-custom-properties": "^12.1.7",
    "postcss-import": "^14.1.0",
    "postcss-loader": "^6.2.1",
    "postcss-preset-env": "^7.4.3",
    "resolve-url-loader": "^5.0.0",
    "sass-loader": "^12.1.0",
    "tailwindcss": "^3.0.23",
    "terser-webpack-plugin": "^4.2.3",
    "webpack": "^5.72.1",
    "webpack-cli": "^4.9.2",
    "webpack-manifest-plugin": "^5.0.0"
  }
}
